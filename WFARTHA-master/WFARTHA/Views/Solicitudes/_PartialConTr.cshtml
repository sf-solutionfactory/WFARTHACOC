                @Html.EditorFor(m => m.DOCUMENTOP[i].POS)
            </td>
            <td>
                @* Numero Anexo *@
            </td>
            <td>
                @Html.EditorFor(m => m.DOCUMENTOP[i].ACCION)
            </td>
            <td>
                @Html.EditorFor(m => m.DOCUMENTOP[i].FACTURA)
            </td>
            <td>
                @Html.EditorFor(m => m.DOCUMENTOP[i].TCONCEPTO)
            </td>
            <td>
                @Html.EditorFor(m => m.DOCUMENTOP[i].GRUPO)
            </td>
            <td>
                @Html.EditorFor(m => m.DOCUMENTOP[i].CUENTA)
            </td>
            <td>
                @Html.EditorFor(m => m.DOCUMENTOP[i].NOMCUENTA)
            </td>
