                @Html.EditorFor(m => m.DOCUMENTOP[i].TIPOIMP)
            </td>
            <td>
                @Html.EditorFor(m => m.DOCUMENTOP[i].IMPUTACION)
            </td>
            <td>
                @Html.EditorFor(m => m.DOCUMENTOP[i].CCOSTO)
            </td>
            <td>
                @Html.EditorFor(m => m.DOCUMENTOP[i].MWSKZ)
            </td>
            <td>
                @Html.EditorFor(m => m.DOCUMENTOP[i].MONTO)
            </td>
            <td>
                @Html.EditorFor(m => m.DOCUMENTOP[i].IVA)
            </td>
            <td>
                @Html.EditorFor(m => m.DOCUMENTOP[i].TEXTO)
            </td>
